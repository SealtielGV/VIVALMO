<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record model="ir.ui.view" id="sale_order_inherit_addenda_extra_fields_form">
            <field name="name">sale.order.inherit.addenda.extra.fields.form</field>
            <field name="model">sale.order</field>
            <field name="inherit_id" ref="nama_modul.sale_order_inherit_addenda_extra_fields_form"/>
            <field name="arch" type="xml">
                <xpath expr="//tree//field[@name='qty_invoiced']" position="after">
                    <field name="prepact_cant"  attrs="{ 'column_invisible': [('parent.partner_id','=', 214)}"/>
                    <field name="pallet_quantity"  attrs="{ 'column_invisible': [('parent.partner_id','=', 214)}"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>